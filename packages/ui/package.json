  "devDependencies": {
    "@acme/eslint-config": "workspace:*",
    "@acme/typescript-config": "workspace:*",
    "@acme/vitest-config": "workspace:*",
    "@testing-library/react": "^16.2.0",
    "@turbo/gen": "^2.4.4",
    "@types/node": "^22.13.9",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitest/coverage-istanbul": "^3.0.8",
    "eslint": "^9.22.0",
    "vitest": "^3.0.8"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}